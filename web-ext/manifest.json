{
  "description": "Kobold web monitoring tool",
  "manifest_version": 2,
  "name": "Kobold monitor",
  "version": "1.0",
  "homepage_url": "https://github.com/juliangrigera/RemoteUsabilityTests",
  "icons": {
    "48": "resources/border-48.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "node_modules/webextension-polyfill/dist/browser-polyfill.min.js",
      "node_modules/axios/dist/axios.min.js",
      "shared/Facade.js",
      "background/BackgroundFacade.js",
      "background/ContentProxy.js",
      "background/ServerAPI.js",
      "background/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "node_modules/webextension-polyfill/dist/browser-polyfill.min.js",
        "node_modules/jquery/dist/jquery.min.js",
        "content_script/BackgroundProxy.js",
        "content_script/InteractionEventMonitor.js",
        "content_script/ConsoleInteractionLogger.js",
        "content_script/XMLHttpRequestInteractionLogger.js",
        "content_script/ClickAttemptMonitor.js",
        "content_script/content.js"
      ]
    }
  ]
}