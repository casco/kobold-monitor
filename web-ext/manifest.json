{
  "description": "Kobold monitorignextension",
  "manifest_version": 2,
  "name": "Kobold monitor extension",
  "version": "1.0",
  "homepage_url": "https://autorefactoring.lifia.info.unlp.edu.ar/",
  "permissions": [
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "node_modules/webextension-polyfill/dist/browser-polyfill.min.js",
      "node_modules/axios/dist/axios.min.js",
      "shared/Facade.js",
      "background/BackgroundFacade.js",
      "background/ContentProxy.js"
    ]
  },

  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "node_modules/webextension-polyfill/dist/browser-polyfill.min.js",
        "node_modules/jquery/dist/jquery.min.js",
        "shared/Facade.js",
        "content_script/ContentFacade.js",
        "content_script/BackgroundProxy.js"
      ]
    }
  ]
}